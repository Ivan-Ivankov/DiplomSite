<header>
  <nav>
    <ul class="semi-bold fs18">
      <li><a href="/">Главная</a></li>
      <li><a href="/about/company">О компании</a></li>
      <li><a href="/about/contacts">Контакты</a></li>
      <% if (locals.user) { %>
      <li>
        <a href="/forUsers/documents">Документы</a>
      </li>
      <% } %>
    </ul>
    <% if (locals.user) { %>
    <div class="header-menu semi-bold">
      <p>Вы вошли как:<span class="login"> <%= user.login %> </span></p>
